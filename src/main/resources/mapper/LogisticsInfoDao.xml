<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="cn.net.yto.dao.LogisticsInfoDao">

    <resultMap id="BaseResultMap" type="cn.net.yto.entity.LogisticsInfo">
        <!--@Table logistics_info-->
        <result property="liid" column="liid" jdbcType="INTEGER"/>
        <result property="wnumber" column="wnumber" jdbcType="VARCHAR"/>
        <result property="wtime" column="wtime" jdbcType="VARCHAR"/>
        <result property="winfo" column="winfo" jdbcType="VARCHAR"/>
    </resultMap>

    <!--查询单个-->
    <select id="queryById" resultMap="BaseResultMap">
        select
          liid, wnumber, wtime, winfo
        from yto.logistics_info
        where liid = #{liid}
    </select>

    <select id="LogisticsInfo" resultMap="BaseResultMap">
        select
          liid, wnumber, wtime, winfo
        from yto.logistics_info
        where wnumber = #{wnumber}
    </select>


    <!--查询指定行数据-->
    <select id="queryAllByLimit" resultMap="BaseResultMap">
        select
          liid, wnumber, wtime, winfo
        from yto.logistics_info
        limit #{offset}, #{limit}
    </select>

    <!--通过实体作为筛选条件查询-->
    <select id="queryAll" resultMap="BaseResultMap">
        select
          liid, wnumber, wtime, winfo
        from yto.logistics_info
        <where>
            <if test="liid != null">
                and liid = #{liid}
            </if>
            <if test="wnumber != null and wnumber != ''">
                and wnumber = #{wnumber}
            </if>
            <if test="wtime != null and wtime != ''">
                and wtime = #{wtime}
            </if>
            <if test="winfo != null and winfo != ''">
                and winfo = #{winfo}
            </if>
        </where>
    </select>

    <!--新增所有列-->
    <insert id="insert" keyProperty="liid" useGeneratedKeys="true">
        insert into yto.logistics_info(wnumber, wtime, winfo)
        values (#{wnumber}, #{wtime}, #{winfo})
    </insert>

    <!--通过主键修改数据-->
    <update id="update">
        update yto.logistics_info
        <set>
            <if test="wnumber != null and wnumber != ''">
                wnumber = #{wnumber},
            </if>
            <if test="wtime != null and wtime != ''">
                wtime = #{wtime},
            </if>
            <if test="winfo != null and winfo != ''">
                winfo = #{winfo},
            </if>
        </set>
        where liid = #{liid}
    </update>

    <!--通过主键删除-->
    <delete id="deleteById">
        delete from yto.logistics_info where liid = #{liid}
    </delete>

</mapper>